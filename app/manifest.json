{
  "name": "airbnb-compare",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds postings' comparison feature to airbnb.com.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.airbnb.com/*",
        "https://*.airbnb.com/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "http://*/*",
    "https://*/*"
  ]
}
